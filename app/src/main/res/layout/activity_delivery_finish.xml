<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.sam.pizza.DeliveryActivities.FinishedOrderedActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="8dp">
        <Button
            android:id="@+id/delivery_log_out_button2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Log out"
            android:textAllCaps="true"
            android:background="@color/mainBlack"
            android:textColor="@color/mainWhite"
            android:layout_alignParentRight="true"
            android:textSize="16dp"/>
        <Button
            android:id="@+id/delivery_orders_list_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" Delivery List "
            android:textAllCaps="true"
            android:layout_below="@id/delivery_log_out_button2"
            android:textSize="16dp"
            android:background="@color/mainBlack"
            android:textColor="@color/mainWhite"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" Finished Orders "
            android:textAllCaps="true"
            android:layout_alignParentRight="true"
            android:layout_below="@id/delivery_log_out_button2"
            android:layout_marginTop="10dp"
            android:textColor="@color/mainBlack"
            android:textSize="24dp"/>
    </RelativeLayout>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="May 2017"
        android:textAllCaps="true"
        android:layout_margin="8dp"
        android:textColor="@color/mainBlack"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/mainOrange"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="16dp">
        <TextView
            android:id="@+id/order_text1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="#6432645\n12/5-12:30"
            android:textColor="@color/mainWhite"
            android:layout_margin="8dp"/>
        <TextView
            android:id="@+id/order_text2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/successful_delivery"
            android:textColor="@color/mainWhite"
            android:layout_toRightOf="@id/order_text1"
            android:layout_marginLeft="100dp"
            android:layout_marginTop="16dp"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/order_text2"/>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/mainOrange"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="8dp">
        <TextView
            android:id="@+id/order_text3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="#6432630\n10/5-15:15"
            android:textColor="@color/mainWhite"
            android:layout_margin="8dp"/>
        <TextView
            android:id="@+id/order_text4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/successful_delivery"
            android:textColor="@color/mainWhite"
            android:layout_toRightOf="@id/order_text3"
            android:layout_marginLeft="100dp"
            android:layout_marginTop="16dp"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/order_text4"/>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/mainOrange"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="8dp">
        <TextView
            android:id="@+id/order_text5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="#6432625\n7/5-9:5"
            android:textColor="@color/mainWhite"
            android:layout_margin="8dp"/>
        <TextView
            android:id="@+id/order_text6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/successful_delivery"
            android:textColor="@color/mainWhite"
            android:layout_toRightOf="@id/order_text5"
            android:layout_marginLeft="100dp"
            android:layout_marginTop="16dp"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/order_text6"/>
    </RelativeLayout>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="april 2017"
        android:textAllCaps="true"
        android:layout_margin="8dp"
        android:textColor="@color/mainBlack"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/mainBlack"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="8dp">
        <TextView
            android:id="@+id/order_text7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="#6432565\n14/4"
            android:textColor="@color/mainWhite"
            android:layout_margin="8dp"/>
        <TextView
            android:id="@+id/order_text8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/successful_delivery"
            android:textColor="@color/mainWhite"
            android:layout_toRightOf="@id/order_text7"
            android:layout_marginLeft="100dp"
            android:layout_marginTop="16dp"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/order_text8"/>
    </RelativeLayout>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="March 2017"
        android:textAllCaps="true"
        android:layout_margin="8dp"
        android:textColor="@color/mainBlack"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/mainBlack"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="8dp">
        <TextView
            android:id="@+id/order_text9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="#6432460\n24/3"
            android:textColor="@color/mainWhite"
            android:layout_margin="8dp"/>
        <TextView
            android:id="@+id/order_text10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/successful_delivery"
            android:textColor="@color/mainWhite"
            android:layout_toRightOf="@id/order_text9"
            android:layout_marginLeft="100dp"
            android:layout_marginTop="16dp"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/order_text10"/>
    </RelativeLayout>

</LinearLayout>
